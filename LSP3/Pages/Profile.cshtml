<style type="text/css">
    * {
        line-height: 1.2em;
    }

    input {
        height: 35px;
        width: 300px;
    }

    .textInput {
        height: 30px;
        width: 300px;
        top: -8px;
    }

    select {
        top: -5px;
        height: 21px;
        margin-left: 0px;
    }

    td {
        height: 20px;
    }

    #MainContent_chkTandC {
        width: 20px;
        height: 10px;
    }

    .error {
        color: red;
    }


    thead tr th:first-child,
    tbody tr td:first-child {
        width: 150px;
        min-width: 150px;
        max-width: 150px;
        word-break: break-all;
    }

    thead tr th:nth-child(-n + 2),
    tbody tr td:nth-child(-n + 2) {
        width: 200px;
        min-width: 200px;
        max-width: 200px;
        word-break: break-all;
        height:35px;
    }


</style>

